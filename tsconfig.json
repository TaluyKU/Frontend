{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "#src/*": [
        "./src/*"
      ],
      "#assets/*": [
        "./assets/*"
      ],
      "#hooks/*": [
        "./src/hooks/*"
      ],
      "#interfaces/*": [
        "./src/interfaces/*"
      ],
      "#utils/*": [
        "./src/utils/*"
      ],
      "#components/*": [
        "./src/components/*"
      ],
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
]
}
